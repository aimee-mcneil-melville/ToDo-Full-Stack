{
  "name": "charlottes-web-log-api",
  "version": "0.2.0",
  "description": "A spider's blog about her web",
  "repository": "https://github.com/dev-academy-challenges/charlottes-web-log-api.git",
  "main": "server/index.js",
  "author": "Dev Academy Aotearoa",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./client/webpack.config.js",
    "dev:client": "npm run build -- --watch",
    "dev:server": "nodemon server",
    "dev:all": "run-p dev:client dev:server",
    "dev": "nodemon server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint . --fix",
    "start": "node server",
    "test": "jest"
  },
  "keywords": [
    "fullstack"
  ],
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "sqlite3": "^5.1.2",
    "express": "^4.17.2",
    "knex": "^2.3.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "superagent": "7.1.1",
    "supertest": "^6.2.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "@babel/preset-typescript": "^7.17.12",
    "@devacademy/eslint-config": "^1.5.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}
